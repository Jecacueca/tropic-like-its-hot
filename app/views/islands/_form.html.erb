<%= simple_form_for [ @island ] do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Submit", class: "btn-search btn-search-index btn-island-new"%>
<% end %>
